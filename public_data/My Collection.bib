journal = {Journal of Wildlife Management},
keywords = {basking-site restoration,crypsis,eastern massasauga rattlesnake,factor influencing habitat,habitat management,new york,operative,reinert 1993,selection in reptiles,sistrurus catenatus,stevenson 1985,temperature,thermoregulation,thermoregulation is a key},
number = {3},
pages = {504--513},
title = {{Evaluating Basking-Habitat Deficiency in the Threatened Eastern Massasauga Rattlesnake}},
volume = {74},
year = {2010}
}
