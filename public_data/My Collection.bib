journal = {Journal of Herpetology},
keywords = {Animal wintering,Habitats,Reptile conservation,Reptile populations,Reptile surveys,Snakes,United States},
month = {mar},
number = {1},
pages = {73--88},
title = {{Occupancy of Potential Overwintering Habitat on Protected Lands by Two Imperiled Snake Species in the Coastal Plain of the Southeastern United States.}},
url = {10.1670/15-064 http://ezproxy.lib.apsu.edu/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=bai&AN=120668496&site=ehost-live&scope=site},
