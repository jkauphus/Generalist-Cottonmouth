arxivId = {_barata Materials and Techniques of polychrome wooden sculpture},
author = {Beck, Daniel D},
doi = {10.1039/c4cp03890h},
eprint = {_barata Materials and Techniques of polychrome wooden sculpture},
file = {:C\:/Users/Jackk/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Beck - 1995 - Ecology and Energetics of Three Sympatric Rattlesnake Species in the Sonoran Desert.pdf:pdf},
