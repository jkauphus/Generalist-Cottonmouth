issn = {1944-687X},
journal = {Journal of Fish and Wildlife Management},
keywords = {1,11,2010,2011,3,accepted,citation,department of natural resources,e1944-,habitat conservation,hine cs,horath mm,illinois,in wetlands,journal of fish and,june 2011,managed by the illinois,march 2011,march 7,moist-soil management,public lands,published,published online early,received,seed abundance for waterfowl,september 15,smith rv,stafford jd,waterfowl,wildlife management 2,yetter ap},
