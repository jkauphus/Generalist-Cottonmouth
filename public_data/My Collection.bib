doi = {10.1016/j.jprot.2013.10.036},
file = {:C\:/Users/Jackk/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lomonte et al. - 2014 - Venomics of new world pit vipers Genus-wide comparisons of venom proteomes across agkistrodon.pdf:pdf},
