@article{Smale2013,
abstract = {Species distributions have shifted in response to global warming in all major ecosystems on the Earth. Despite cogent evidence for these changes, the underlying mechanisms are poorly understood and currently imply gradual shifts. Yet there is an increasing appreciation of the role of discrete events in driving ecological change. We show how a marine heat wave (HW) eliminated a prominent habitat-forming seaweed, Scytothalia dorycarpa, at its warm distribution limit, causing a range contraction of approximately 100 km (approx. 5% of its global distribution). Seawater temperatures during the HW exceeded the seaweed's physiological threshold and caused extirpation of marginal populations, which are unlikely to recover owing to life-history traits and oceanographic processes. Scytothalia dorycarpa is an important canopy-forming seaweed in temperate Australia, and loss of the species at its range edge has caused structural changes at the community level and is likely to have ecosystem-level implications. We show that extreme warming events, which are increasing in magnitude and frequency, can force step-wise changes in species distributions in marine ecosystems. As such, return times of these events have major implications for projections of species distributions and ecosystem structure, which have typically been based on gradual warming trends. {\textcopyright} 2013 The Author(s) Published by the Royal Society. All rights reserved.},
author = {Smale, Dan A. and Wernberg, Thomas},
doi = {10.1098/rspb.2012.2829},
file = {:C\:/Users/Jackk/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Smale, Wernberg - 2013 - Extreme climatic event drives range contraction of a habitat-forming species.pdf:pdf},
issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Australia,Climate change,Macroalgae,Marine heat wave,Range shifts,Species distributions},
number = {1754},
title = {{Extreme climatic event drives range contraction of a habitat-forming species}},
volume = {280},
year = {2013}
}
@article{Smith2018,
abstract = {We studied the spatial and reproductive ecology of a population of Copperheads (Agkistrodon contortrix), a North American pitviper, in a basalt trap-rock ecosystem in the central Connecticut River Valley, a region that constitutes the northeastern extreme of this species' geographic range. Adult males (n = 20) and females (n = 15) were surgically implanted with radio-transmitters and tracked every 48 h during the active season (April through October) for three consecutive years (2001 to 2003). From late autumn to early spring (November through March), when snakes were hibernating and thus inactive, tracking was reduced to once per week. We generated data on movement and other spatial parameters for each subject using GPS coordinates. There were significant sex differences in activity range size and multiple movement parameters. Throughout the active season males had greater activity range sizes and showed greater movement than females. This trend was pronounced during the mating season, which was restricted to late summer and early fall (late July through September). In contrast to most populations of A. contortrix from more southern and western localities, we did not observe any sexual activity (e.g., courtship, coitus, and male-male fighting) in the spring. Individuals of both sexes showed annual fidelity to: (i) activity range location, (ii) activity range size, (iii) movement distances, (iv) particular features of their activity ranges (e.g., refuge sites), and (v) hibernation sites. Males and females showed no difference in preferred seasonal habitats. In both sexes, shifts in habitat associations during the active season included migrations from over-wintering sites within basalt trap rockslides to upper-elevation, open deciduous forest during the summer foraging and reproductive season. Parturition in the field was recorded in eight instances and was always close to one of the two hibernacula used by the individuals in the study},
annote = {Investigated the movements and space, sex differences in movements and space use, fidelity to sites used at different times across seasons, including Hibernacula, seasonal timing and duration of mating season, and timing of birth and location of parturition sites. 
Results:
* Males follow similar seasonal activity range and move patt. Emerge between March and April
* the showed that sex differences in activity range and several movement parameters are pronounced. Males maintain activity ranges during the single mating season that were larger than females. Movement distances of males showed similar trends relative to females, esp mating season.
* Suggested that males and females eat similar diets and similar rates of feeding.
* Activity of males increases as they search for females during breeding season.
* Females aggregate during pregnancy as well as reduced movement to near hibernaculum.
* Loyal fidelity to dens.
* Southern and Western A. contortrix mate late summer/ early fall and spring. However the northern pop strictly July/September.
* past reproductive success can influnce site fidelity and activity range fidelity across taxa, and they predict strong selection on males to maintain similar activity locations between years.
* males and females show fidelity to refuge sites along their respective ranges. Refuges such as rock piles, hollow tree trunks logs.
Migration to hib to range takes the shape of a loop pattern. 
*single mating season and male activity patterns are observed in North cottonmouths and congener Cottonmouths.
* Hypo that males compete for sex by scramble usually with rare fights.
* Males and non pregnant females prefer open canopy rocky habitats, talus slopes, and adjacent decidious forest. leaf litter sub and use of open grassy habitats.},
author = {Smith, Charles and Shuett, Gordon and Earley, Ryan and Schwenk, Kurt},
file = {:C\:/Users/Jackk/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Smith et al. - 2018 - The spatial and reproductive ecology of the copperhead (agkistrodon contortrix) at the northeastern extreme of its.pdf:pdf},
journal = {Herpetological Monographs},
keywords = {Activity range,Agkistrodon,Geographic variation,Mating system,Migration,Movement,Reproduction,Reptile,Snakes},
